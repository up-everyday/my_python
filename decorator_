def w1(func):
    def inner(*args, **kwargs):
        print("veify!!!")
        return  func(*args, **kwargs)
    return inner

@w1
def f1(x,y):
    print("f1 ",x, y)
@w1
def f2(z):
    print("f2 ", z,)
@w1
def f3(k=100):
    print("f3", k)
if __name__ == '__main__':
    f1(1, 2)
    f2(3)
    f3(k=200)
